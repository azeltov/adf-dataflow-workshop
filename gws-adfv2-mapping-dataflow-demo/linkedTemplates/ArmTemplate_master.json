{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "gws-adfv2-mapping-dataflow-demo"
		},
		"gws_adls_gen2_dataflow_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'gws_adls_gen2_dataflow'"
		},
		"RawReferenceDataTopDir_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"RawReferenceDataTopDir_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "raw/reference-data"
		},
		"RawTransactionDataTopDir_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"RawTransactionDataTopDir_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "raw/transactional-data"
		},
		"gws_adls_gen2_dataflow_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfmappingdfdemosa.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"gws_adls_gen2_dataflow_accountKey": {
						"value": "[parameters('gws_adls_gen2_dataflow_accountKey')]"
					},
					"gws_adls_gen2_dataflow_properties_typeProperties_url": {
						"value": "[parameters('gws_adls_gen2_dataflow_properties_typeProperties_url')]"
					},
					"RawReferenceDataTopDir_properties_typeProperties_fileName": {
						"value": "[parameters('RawReferenceDataTopDir_properties_typeProperties_fileName')]"
					},
					"RawReferenceDataTopDir_properties_typeProperties_folderPath": {
						"value": "[parameters('RawReferenceDataTopDir_properties_typeProperties_folderPath')]"
					},
					"RawTransactionDataTopDir_properties_typeProperties_fileName": {
						"value": "[parameters('RawTransactionDataTopDir_properties_typeProperties_fileName')]"
					},
					"RawTransactionDataTopDir_properties_typeProperties_folderPath": {
						"value": "[parameters('RawTransactionDataTopDir_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}